<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.wccnet.notificationsdemo">

    <!-- Note what happens when you uncomment this
    <uses-sdk android:minSdkVersion="21"
        android:targetSdkVersion=""
        android:maxSdkVersion="integer" />

        How is this handled when using Gradle?  Where is it handled
    -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NotificationsDemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".OrderSynchService" />
    </application>

</manifest>